{
  "name": "angular2components",
  "version": "1.0.0",
  "description": "This README would normally document whatever steps are necessary to get your application up and running.",
  "main": "./src/index.html",
  "scripts": {
    "develop": "cross-env AOT=false webpack-dev-server --inline --progress --port 8080",
    "build": "rimraf prod && webpack --config config/webpack.prod.js --progress --profile --bail --display-optimization-bailout",
    "aot": "cross-env AOT=true rimraf dist && webpack --config config/webpack.aot.js --progress --profile --bail",
    "serveraot": "cross-env SSR=true rimraf serverSide && webpack --config config/webpack.server.aot.js --progress --profile --bail",
    "serverdist": "node backend/server-dist.js",
    "serverprod": "node backend/server-prod.js",
    "serverssr": "nodemon serverSide/server.js",
    "dev-aot-ssr": "npm run aot && npm run serveraot",
    "parallel-prod": "rimraf dist && rimraf serverSide && parallel-webpack --config=config/webpack.parallel.js",
    "paralleldev": "rimraf dist && rimraf serverSide && parallel-webpack --config=config/webpack.parallel.js --no-stats --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Mechoshi@bitbucket.org/Mechoshi/angular2components.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Mechoshi/angular2components#readme",
  "dependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/compiler-cli": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/platform-server": "^4.3.0",
    "@angular/router": "^4.3.0",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "core-js": "^2.4.1",
    "express": "^4.15.3",
    "jshint": "^2.9.5",
    "rxjs": "^5.4.2",
    "socket.io": "^2.0.3",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "^0.8.13"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.5.1",
    "@types/express": "^4.0.36",
    "@types/node": "^6.0.64",
    "ajv": "^5.2.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "on-build-webpack": "^0.1.0",
    "parallel-webpack": "^2.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^7.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "typescript": "^2.4.1",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "2.5.1",
    "webpack-livereload-plugin": "^0.11.0",
